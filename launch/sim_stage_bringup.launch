<launch>

    <arg name="world_file" value="$(find kezbot_v2)/worlds/stage/one_robot.world" />

    <param name="/use_sim_time" value="true" />

    <node ns="kezbot" pkg="stage_ros" type="stageros" name="stageros"
        args="$(arg world_file)">
        <remap from="odom" to="odom" />
    </node>

</launch>
